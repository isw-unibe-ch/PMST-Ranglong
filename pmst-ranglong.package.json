              "#DEF#"
            ],
            "constraints": {
              "pattern": "(ts|ee|j|k|m|n|r|s|t|ŋ|a|e|i|o|u|Σ)( (ts|ee|j|k|m|n|r|s|t|ŋ|a|e|i|o|u|Σ))*"
